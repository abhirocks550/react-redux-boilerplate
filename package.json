{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "React Redux with React router boilerplate. Eslint for linting code and JEST for testing React.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "lint": "eslint . --ext .js,.jsx",
    "build": "NODE_ENV=production npx parcel build src/index.js -d public/js/ --no-cache",
    "dev": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "Abhijit",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "nodemon": "^1.18.3",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "morgan": "^1.9.0",
    "parcel-bundler": "^1.9.7",
    "pug": "^2.0.3"
  }
